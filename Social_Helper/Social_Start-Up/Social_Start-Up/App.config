<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
  <appSettings>

    <!--How long active-->
    <add key="Hours" value="1"/>
    
      <!--Email-->
    <add key="My_Email" value="xxx@hotmail.com"/>
    
    <!--Twitter-->
    <add key="TwitterURL" value="https://twitter.com/login"/>
    <add key="SendMessage" value="false"/>
    <add key="TwitterMessage" value="Place message here"/>

    <!--PsnProfiles-->
    <add key="UpdateProfile" value="true"/>
    
    <!--Facebook-->
    <add key="FacebookURL" value="https://www.facebook.com/"/>
  
   <!--Passwords-->
    <add key="Password1" value="xxxxxxxxx"/>
    <add key="Password2" value="xxxxxxxxx"/>
    
    <!--YouTube-->
    <add key="Song" value="xxxxxxx"/>
    <add key="RepeatSong" value="true"/>

    <!--LOGOUTS-->
    <add key="SlackLogOut" value="true"/>
    <add key="YouTubeLogOut" value="true"/>
    <add key="LinkedInLogOut" value="true"/>
    <add key="EmailLogOut" value="true"/>
    <add key="TwitterLogOut" value="true"/>
    <add key="FacebookLogOut" value="true"/>

  </appSettings>
</configuration>